// node {
// 	echo "build"
//     echo "test"
//     echo "integration test"
// }
pipeline {
    agent any
    stages{
        stage("build") {
            steps {
                echo "build"
            }
        }
        stage("test") {
            steps {
                echo "test"
            }
        }
        stage("integration test") {
            steps {
                echo "integration"
            }
        }
    } post {
        always {
            echo "i am awesome. run always"
        }
        success {
            echo 'i run success'
        }
        
    }
}